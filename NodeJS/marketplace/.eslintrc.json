{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["standard", "plugin:prettier/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": "error",
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "consistent-return": 0
  }
}
